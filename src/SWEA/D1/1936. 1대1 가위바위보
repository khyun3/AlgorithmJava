		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		if(a+b == 3) {
			if(a==1)
				System.out.println("B");
			else
				System.out.println("A");
		}
		else if(a+b == 4) {
			if(a==1)
				System.out.println("A");
			else
				System.out.println("B");
		}
		else {
			if(a==2)
				System.out.println("B");
			else
				System.out.println("A");
		}