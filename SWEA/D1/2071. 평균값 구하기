		Scanner sc = new Scanner(System.in);
		int T;
		float temp=0;
		T= sc.nextInt();
		int [] result = new int[T];
		
		for(int i=0; i<T; i++) {
			for(int j=0; j<10; j++) {
				temp += sc.nextFloat();
			}
			result[i] = Math.round(temp/10);
			temp = 0;
		}
		for(int i=0; i<T; i++) {
			System.out.println("#" + (i+1) + " " + result[i]);
		}