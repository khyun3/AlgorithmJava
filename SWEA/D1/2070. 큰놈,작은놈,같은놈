		Scanner sc = new Scanner(System.in);
		int T,a=0,b=0;
		T = sc.nextInt();
		char [] result = new char[T];
		for(int i=0; i<T; i++) {
			a = sc.nextInt();
			b = sc.nextInt();
			if(a == b)
				result[i] = '=';
			else if(a > b)
				result[i] = '>';
			else if(a < b)
				result[i] = '<';
			else
				System.out.println("error");
		}
		for(int j=0; j<T; j++) System.out.println("#"+(j+1)+" "+result[j]);